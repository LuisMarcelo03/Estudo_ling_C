#include <stdio.h>

int main(){
	
	int m;
   printf("valor de 1 a 7\n");
   scanf("%d", &m);
      
      
      switch(m){
   	case 1:
   		printf("Domingo\n");
   		break;
		  
	case 2:
	   printf("Segunda\n");
	   break;

	case 3: 
	   printf("ter√ßa\n");
	    break;

	case 4:
	   printf("quarta\n");
	   break;

	case 5:
	    printf("quinta\n");
		break;

    case 6:
	    printf("sexta\n");
		break;

    case 7:
   		printf("sabado\n");
   		break;

	default:
	    printf("Valor invalido\n");
	    break;	   
		   }
   }
